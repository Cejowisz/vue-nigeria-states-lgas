{
  "name": "vue-nigeria-states-lgas",
  "version": "1.0.9",
  "description": "Vue library that lists Nigeria states and LGAs.",
  "main": "dist/vue-nigeria-states-lgas.umd.js",
  "module": "dist/vue-nigeria-states-lgas.esm.js",
  "unpkg": "dist/vue-nigeria-states-lgas.min.js",
  "browser": {
    "./sfc": "src/StatesLgas.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-naija-states-lgas.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-naija-states-lgas.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-naija-states-lgas.min.js"
  },
  "author": "Justin Cejowisz Obi",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-runtime": "^6.9.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.66.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue": "^2.2.1",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^11.1.4",
    "vue-style-loader": "^2.0.3",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^1.13.1",
    "webpack-merge": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cejowisz/vue-nigeria-states-lgas.git"
  },
  "keywords": [
    "Nigeria",
    "States",
    "Country",
    "Local",
    "Government",
    "Nodejs",
    "Vue",
    "JavaScript",
    "Capital",
    "City",
    "Naija",
    "Lga",
    "Local-government"
  ]
}
